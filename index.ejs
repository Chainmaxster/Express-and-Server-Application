<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data Management App</title>
</head>
<body>
    <h1>Welcome to the Data Management App</h1>
    
    <h2>Users</h2>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.name %></li>
        <% }); %>
    </ul>

    <h2>Posts</h2>
    <ul>
        <% posts.forEach(post => { %>
            <li><strong><%= post.title %></strong>: <%= post.content %></li>
        <% }); %>
    </ul>

    <h2>Comments</h2>
    <ul>
        <% comments.forEach(comment => { %>
            <li>Post <%= comment.postId %>: <%= comment.content %></li>
        <% }); %>
    </ul>

    <script src="/script.js"></script>
</body>
</html>